{
  "name": "vite-kuromoji.ts",
  "description": "TypeScript implementation of Japanese morphological analyzer",
  "type": "module",
  "version": "0.1.2",
  "author": "Takuya Asano <takuya.a@gmail.com>",
  "main": "./build/kuromoji.js",
  "source": "./src/kuromoji.ts",
  "types": "./build/kuromoji.d.ts",
  "keywords": [
    "japanese",
    "morphological analyzer",
    "nlp",
    "pos",
    "pos tagger",
    "tokenizer"
  ],
  "license": "Apache-2.0",
  "scripts": {
    "build": "parcel build",
    "build-dict": "vite build --mode dict",
    "coverage": "vitest run --coverage",
    "lint": "biome check ./src",
    "test": "vitest",
    "watch": "vite build --watch",
    "example:deno": "deno run -A --unstable-sloppy-imports ./example/load-deno.js",
    "example:node": "tsx ./example/load-node.js",
    "example:bun": "bun run ./example/load-bun.js"
  },
  "engines": {
    "node": ">= 12"
  },
  "devDependencies": {
    "@biomejs/biome": "1.9.4",
    "@parcel/packager-ts": "2.13.3",
    "@parcel/transformer-typescript-types": "2.13.3",
    "@vitejs/plugin-legacy": "^6.0.1",
    "@vitest/coverage-v8": "^3.0.6",
    "buffer": "^6.0.0",
    "parcel": "^2.0.0",
    "tsx": "^4.19.3",
    "typescript": "^5.7.3",
    "vite": "^6.1.0",
    "vite-plugin-checker": "^0.8.0",
    "vitest": "^3.0.6"
  },
  "dependencies": {
    "vite-kuromoji.ts": "git+https://github.com/materialfill65/vite-kuromoji.ts.git"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "@biomejs/biome",
      "@parcel/watcher",
      "@swc/core",
      "core-js",
      "esbuild",
      "lmdb",
      "msgpackr-extract"
    ]
  }
}
